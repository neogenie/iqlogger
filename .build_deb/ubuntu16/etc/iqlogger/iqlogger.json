{
  "iqlogger" : {
    "logger" : {
      "filename": "/var/log/iqlogger/iqlogger.log",
      "level" : "info"
    },
    "stats" : {
      "interval": 1,
      "export" : [
        {
          "name": "http",
          "type": "http",
          "listen_host": "0.0.0.0",
          "listen_port": 8888
        }
      ]
    },
    "processor" : {
      "threads": 1
    }
  },
  "input" : {
    "threads": 1,
    "sources": [
      {
        "name": "journal",
        "type": "journal",
        "threads": 1,
        "processor" : "processor.js"
      }
    ]
  },
  "output" : {
    "threads": 1,
    "destinations" : [
      {
        "name": "gelf_tcp",
        "type" : "gelf",
        "protocol" : "tcp",
        "port" : 12202,
        "hosts" : [
          "graylog.com"
        ],
        "timeout" : 30,
        "threads": 2
      }
    ]
  }
}